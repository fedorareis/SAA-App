<test name="One Plane In Airspace Correlation"
      description="Detect one plane on three sensors, and correlate all the results"
      time = "40"
      sensorError = "false">
   <ownship tag="ABC123">
      <tcas enabled="true" error="1.5"/>
      <radar enabled="true"/>
      <ads-b enabled="true" error="0.2"/>
      <movement type="linear">
         <position x="0" y="0" z="0"/>
         <direction x="800" y="0" z="0"/>
      </movement>
   </ownship>
   <plane tag="N37789">
      <ads-b enabled="true"/>
      <tcas enabled="true"/>
      <movement type="linear">
         <position x="0" y="10" z="0.3"/> <!--NED in Nautical Miles-->
         <direction x="800" y="0" z="0"/> <!--Feet/second-->
      </movement>
   </plane>
</test>

