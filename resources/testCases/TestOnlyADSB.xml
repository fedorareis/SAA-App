<test name="Only ADSB"
      description="Tests an intruder with only ADSB."
      time = "30"
      sensorError = "false">
   <ownship tag="ABC123">
      <tcas enabled="true" error="1.5"/>
      <radar enabled="true"/>
      <ads-b enabled="true" error="0.2"/>
      <movement type="linear">
         <position x="0" y="0" z="1.0"/>
         <direction x="300" y="0" z="0"/>
      </movement>
   </ownship>
   <plane tag="N37789">
      <ads-b enabled="true" error="0.2"/>
      <movement type="linear">
         <position x="0" y="10" z="1.5"/> <!--NED in Nautical Miles-->
         <direction x="300" y="300" z="-200"/> <!--Feet/second-->
      </movement>
   </plane>
   <plane tag="B13423">
      <ads-b enabled="true" error="0.2"/>
      <movement type="linear">
         <position x="-8" y="18" z="0.5"/> <!--NED in Nautical Miles-->
         <direction x="200" y="400" z="-100"/> <!--Feet/second-->
      </movement>
   </plane>
</test>

